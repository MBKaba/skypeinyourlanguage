msgid "New contact request"
msgstr "ߟߊ߬ߛߐ߬ߘߐ߲߬ߕߊ߬ ߡߊߞߟߌߟߌ߫ ߞߎߘߊ."

msgctxt "sLIVE_B_FULLSCREEN"
msgid "Full screen"
msgstr "ߥߊ߲߬ߥߊ߲߬ߝߊ"

msgctxt "sLIVE_B_FULLSCREENEXIT"
msgid "Exit fullscreen"
msgstr "ߓߐ߫ ߥߊ߲߬ߥߊ߲߬ߝߊ ߘߐ߫"

msgctxt "sLIVE_B_ACTUALSIZE"
msgid "Actual size"
msgstr "ߟߐ߬ߜߊ߲߬ ߖߏߣߡߊ"

msgctxt "sLIVE_B_NONACTUALSIZE"
msgid "Fit screen"
